{
    "name": "pantheon-upstreams/drupal-project",
    "description": "Install Drupal 9 with Composer on Pantheon.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "furf/jquery-ui-touch-punch",
                "version": "2.0",
                "minimum-stability": "stable",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/furf/jquery-ui-touch-punch/archive/master.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.1",
        "drupal/better_exposed_filters": "^5.0",
        "drupal/block_visibility_groups": "^1.4",
        "drupal/bootstrap": "^3.23",
        "drupal/bootstrap4_modal": "^2.1",
        "drupal/clientside_validation": "^3.0@RC",
        "drupal/config_ignore": "^3.0@beta",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-recommended": "^9.2",
        "drupal/devel": "^4.1",
        "drupal/devel_kint_extras": "^1.0",
        "drupal/disable_messages": "^2.0",
        "drupal/email_registration": "^1.1",
        "drupal/encrypt": "^3.0",
        "drupal/entity_redirect": "^2.0",
        "drupal/entityform_block": "^1.0@alpha",
        "drupal/feeds": "^3.0@alpha",
        "drupal/feeds_tamper": "^2.0@beta",
        "drupal/field_permissions": "^1.1",
        "drupal/field_validation": "^1.0@beta",
        "drupal/form_mode_control": "^2.0",
        "drupal/ief_table_view_mode": "^2.2",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/inline_formatter_field": "^2.1",
        "drupal/layout_builder_modal": "^1.1",
        "drupal/link_target": "^1.3",
        "drupal/login_destination": "^2.0@beta",
        "drupal/menu_export": "^1.3",
        "drupal/menu_link_attributes": "^1.2",
        "drupal/menu_per_role": "^1.4",
        "drupal/menus_attribute": "^1.2",
        "drupal/modules_weight": "^1.9",
        "drupal/nodeaccess": "^1.1",
        "drupal/otp": "^1.0@alpha",
        "drupal/paragraphs": "^1.12",
        "drupal/password_eye": "^2.0",
        "drupal/password_policy": "^3.1",
        "drupal/phpmailer_smtp": "^2.0",
        "drupal/r4032login": "^2.2",
        "drupal/real_aes": "^2.4",
        "drupal/recaptcha": "^3.0",
        "drupal/redirect404_home": "^1.0",
        "drupal/redirect_after_login": "^2.7",
        "drupal/swiftmailer": "^2.2",
        "drupal/tamper": "^1.0@alpha",
        "drupal/telephone_validation": "^2.3",
        "drupal/token": "^1.10",
        "drupal/view_password": "^6.0",
        "drupal/views_autocomplete_filters": "^1.3",
        "drupal/views_bulk_operations": "^4.1",
        "drupal/views_data_export": "^1.1",
        "drupal/views_entity_form_field": "^1.0@beta",
        "drupal/we_megamenu": "^1.13",
        "drupal/webform": "^6.2@beta",
        "drupal/webform_views": "^5.0@beta",
        "drush/drush": "^10",
        "furf/jquery-ui-touch-punch": "^2.0",
        "kint-php/kint": "^3.3",
        "vlucas/phpdotenv": "^5.4"
    },
    "require-dev": {
        "drupal/core-dev": "^9.2"
    },
    "conflict": {
            "drupal/drupal": "*"
    },
    "autoload": {
        "files": ["load.environment.php"]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "allowed-packages": [
                "pantheon-systems/drupal-integrations"
            ],
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/pantheon.upstream.yml": false,
                "[project-root]/.gitattributes": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "web/private/scripts/quicksilver/{$name}/": ["type:quicksilver-script"]
        },
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "#2951547: Fix issue with layout overflow": "https://www.drupal.org/files/issues/2020-01-08/layout-builder-tray-size-2951547-42.patch",
                "#2761273: Views exposed filter tokens": "https://www.drupal.org/files/issues/2020-03-02/exposed_filter_tokens-2761273-21.patch",
                "#2985882: Workaround for Call to a member function getLabel() after enabling layout_builder": "https://www.drupal.org/files/issues/2020-04-08/2985882-field-85.patch",
                "2880011: Add a #disable_inline_form_errors_summary property to disable the Inline Form Errors summary": "https://www.drupal.org/files/issues/2021-08-23/2880011-85.patch",
                "3109650: Refactor Xss::attributes() to allow filtering of style attribute values": "https://www.drupal.org/files/issues/2021-06-06/3109650-36.patch"

            },
            "drupal/disable_messages": {
                "2901390#33: Non-existent service path.alias_manager": "https://www.drupal.org/files/issues/2021-01-21/3193625-5.patch"
            },
            "drupal/paragraphs": {
                "2901390#33: fixing issue langcode cannot be null": "https://www.drupal.org/files/issues/2019-08-10/paragraphs-set_langcode_widgets-290139_updated.patch"
            },
            "drupal/recaptcha": {
                "2493183: Ajax support / Use behaviors for 2.x": "https://www.drupal.org/files/issues/2021-06-03/recaptcha-ajax-2493183-225.patch"
            },
            "drupal/swiftmailer": {
                "3189973: Support HTML in email body.": "https://www.drupal.org/files/issues/2020-12-26/support_html_in_message_body-3189973.patch"

            },
            "drupal/clientside_validation": {
                "3200530: clientside_validation.": "https://www.drupal.org/files/issues/2021-10-28/3200530-9_17.patch"

            }


        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true
        }
    }
}
